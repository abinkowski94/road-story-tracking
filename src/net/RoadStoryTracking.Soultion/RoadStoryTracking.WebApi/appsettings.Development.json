{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\SQLEXPRESS;Database=RoadStoryTracking;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Storage": {
    "ImageBlobStorageName": "Default",
    "Blobs": {
      "Default": {
        "ConnectionString": "UseDevelopmentStorage=true",
        "DefaultContainerName": "city-story-tracking",
        "Location": "assets\\markers\\images"
      }
    },
    "Queues": {
      "Default": {
        "ConnectionString": "UseDevelopmentStorage=true",
        "QueueName": "images-queue"
      }
    }
  }
}