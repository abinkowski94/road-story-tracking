{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Images": {
    "Small": {
      "Width": 320,
      "Height": 180
    },
    "Medium": {
      "Width": 1366,
      "Height": 768
    },
    "Large": {
      "Width": 1920,
      "Height": 1080
    }
  },
  "Storage": {
    "DefaultQueue": {
      "ConnectionString": "UseDevelopmentStorage=true"
    },
    "Blobs": {
      "Default": {
        "ConnectionString": "UseDevelopmentStorage=true",
        "DefaultContainerName": "city-story-tracking",
        "Location": "assets\\markers\\images"
      },
      "Test": {
        "ConnectionString": "UseDevelopmentStorage=true",
        "DefaultContainerName": "city-story-tracking",
        "Location": "testAssets\\markers\\images"
      }
    }
  }
}