{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Storage": {
    "DefaultQueue": {
      "ConnectionString": "UseDevelopmentStorage=true"
    },
    "Blobs": {
